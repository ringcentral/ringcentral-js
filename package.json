{
  "name": "rcsdk",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "build": "./node_modules/.bin/gulp",
    "watch": "npm run build -- watch",
    "test": "./node_modules/.bin/mocha --require ./test/mocha --recursive ./lib/**/*.js",
    "test-api": "./node_modules/.bin/mocha --require ./test/mocha-api --recursive ./test/specs-api/**/*.js",
    "karma": "npm run build -- --skipTests && ./node_modules/karma/bin/karma start karma.conf.js",
    "karma-chrome": "npm run karma -- --browsers Chrome",
    "karma-watch": "npm run karma-chrome -- --no-single-run --auto-watch --log-level warn",
    "istanbul": "./node_modules/.bin/istanbul cover -x *-spec.js --dir=./build/coverage --report=lcov --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --require ./test/mocha.js --reporter spec lib/**/*-spec.js"
  },
  "dependencies": {
    "crypto-js": "*",
    "dom-storage": "*",
    "es6-promise": "*",
    "pubnub": "*",
    "requirejs": "*",
    "xhr2": "*"
  },
  "devDependencies": {
    "amd-optimize": "*",
    "amdclean": "*",
    "chai": "*",
    "chai-spies": "*",
    "express": "*",
    "gulp": "*",
    "gulp-add-src": "*",
    "gulp-amdclean": "*",
    "gulp-concat": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-sourcemaps": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "gulp-wrap-js": "*",
    "istanbul": "*",
    "karma": "*",
    "karma-coverage": "*",
    "karma-html-reporter": "*",
    "karma-mocha": "*",
    "karma-mocha-reporter": "*",
    "karma-phantomjs-launcher": "*",
    "karma-requirejs": "*",
    "karma-sinon-chai": "*",
    "mocha": "*",
    "soap": "*"
  },
  "preferGlobal": false,
  "private": true,
  "main": "./lib/index.js",
  "author": {
    "name": "RingCentral, Inc.",
    "email": "devsupport@ringcentral.com"
  },
  "contributors": [
    {"name": "Kirill Konshin"}
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ringcentral/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ringcentral/js-sdk/issues"
  },
  "homepage": "https://github.com/ringcentral/js-sdk",
  "engines": {
    "node": "0.10.x"
  },
  "license": "http://www.ringcentral.com/legal/apilitos.html"
}
